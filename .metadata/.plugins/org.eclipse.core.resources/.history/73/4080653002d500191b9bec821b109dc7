package solution_tutorial2;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.Queue;
import java.util.Scanner;

public class week2_3_1ver {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String str = null, goStop = "go";
		String[] recode = new String[2];
		Queue<String> guest = new LinkedList<String>();
		ArrayList<String> food = new ArrayList<String>();

		int pi = 0, ham = 0, any = 0;

		while (true) {
			str = input.nextLine();
			recode = str.trim().split(" ");
			if(recode[0].contentEquals("E"))
				break;
			if (recode[0].equals('O') && guest.isEmpty()) {// 손님이 없음
				for(int i =0; i < food.size(); i++) {
					if(recode[1].equals(food.indexOf(i))) {
						if(recode[1].equals("Pizza")) {
							pi++;
							food.remove(i);}
						else if(recode[1].equals("Hamburger")) {
							ham++;
							food.remove(i);}
						else {								//any요구 처리
							if(food.remove(0)=="Pizza")
								pi++;
							else
								ham++;
						}
					}
				}
			} else if (recode[0].equals('O') &&guest.isEmpty()&&food.isEmpty()) {// 손님이 없고 음식도 없음
				guest.offer(recode[1]);
			} else if (recode[0].equals('O') && guest.size()<2&&food.isEmpty()) {// 주문자가 2명이 기다림 즉, 손님 걍 감.
				guest.offer(recode[1]);
			}else if(recode[0].equals('O') && guest.size()==2&&food.isEmpty())
				any++;
			if (recode[0].equals('F')&&food.isEmpty()) {// 아무것도 없을때 조리 완료 
				if(!guest.isEmpty()&&guest.peek().equals(recode[1])) {// 손님 요구와 같아 없어짐. any 포함.
					if(recode[1].equals("Pizza"))
						pi++;
					else if(recode[1].equals("Hamburger"))
						ham++;
					guest.poll();
				} else if(!guest.isEmpty()&&!guest.peek().equals(recode[1])) {// 손님 요구와 같지않아 저장됨.
					food.add(recode[1]);
				} else if(guest.isEmpty()) //음식 없음 but 요구자가 없음
					food.add(recode[1]);
				
			} else if(recode[0].equals('F')&&!food.isEmpty()) { // 요리가 있지만 여지껏 요구에 맞지 않았음
				if(!guest.isEmpty()&&guest.peek().equals(recode[1])) {// 손님 요구와 같아 없어짐. any 포함.
					if(recode[1].equals("Pizza"))
						pi++;
					else if(recode[1].equals("Hamburger"))
						ham++;
					guest.poll();
				} else if(!guest.isEmpty()&&!guest.peek().equals(recode[1])) {// 손님 요구와 같지않아 저장됨.
					food.add(recode[1]);
				}else if(guest.isEmpty()) //음식 있음 but 요구자가 없음
					food.add(recode[1]);
			}
		}
		System.out.println("Pizza: " + pi + "\nHamburger: " + ham + "\nNoting: " + any);
		input.close();
	}

}
