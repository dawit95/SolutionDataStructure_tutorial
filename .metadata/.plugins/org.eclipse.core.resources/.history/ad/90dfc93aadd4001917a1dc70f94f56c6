package solution_tutorial2;

import java.util.Scanner;

public class week2_2 {
	public static int cardOrder(String[] str) {
		int pocard=0, triple=0, tupair=0;
		int[] alp = new int[26];

		for (int i = 0; i < alp.length; i++)
			alp[i] = 0;

		for (int i = 0; i < str.length; i++) {
			alp[i] = "A".compareTo(str[i]);
		}
		
		for(int i = alp.length-1; i>=0; i--) {
			if(alp[i]==4&&pocard==0)
				pocard=i;
			else if(alp[i]==3&&triple==0)
				triple=i;
			else if(alp[i]==2&&tupair==0)
				tupair=i;
			else if(pocard==0&&triple==0&&tupair==0)
				;
		}
	}

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String table = null;
		String[] card = new String[10];
		String[] tom = new String[5], jerry = new String[5];

		while (input.hasNextLine()) {
			table = input.nextLine();
			card = table.trim().split("");
			for (int i = 0; i < 5; i++) {
				tom[i] = card[i];
				jerry[i + 5] = card[i + 5];
			}
			
		}
		input.close();
	}

}
